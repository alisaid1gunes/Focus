{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
    "prettier/prettier": [
          "error",
          {
            "singleQuote": true,
            "semi": true,
            "no-console": "off",
            "func-names": "off",
            "no-process-exit": "off",
            "object-shorthand": "off",
            "class-methods-use-this": "off",
            "endOfLine":"auto",
            "no-underscore-dangle": "off"
           
          }
        ]
      }
  }